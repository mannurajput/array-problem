//insert element in array
let arr=[77,88,99,66]
let position=2;
let num=1000;
for(let i=arr.length-1;i>=0;i--){
if(i>=position){
arr[i+1]=arr[i]
if(i==position){
arr[i]=num
}
}
}console.log(arr)


     //or



let arr=[8,3,4,5]
let position=3;
let num =100000;
for(let i=0;i<=position;i++){
if(i<=position){
arr[i]=arr[i+1]
if(i==position){
arr[i]=num
}
}
}
console.log(arr)
